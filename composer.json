{
  "name": "gertvdb/suggestions",
  "description": "Helper for theme suggestions.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "minimum-stability": "alpha",
  "autoload": {
    "psr-4": {
      "Drupal\\suggestions\\": "src/"
    }
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "php": ">=8.1",
    "drupal/hook_event_dispatcher": "^4.0",
    "gertvdb/preprocess_form_element_event_dispatcher": "^3.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "drupal/core-recommended": "^10.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}

{
  "name": "gertvdb/suggestions",
  "description": "Helper for theme suggestions.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "minimum-stability": "alpha",
  "autoload": {
    "psr-4": {
      "Drupal\\hook_event_dispatcher\\": "src/"
    }
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  },
  "require": {
    "php": ">=8.1",
    "drupal/hook_event_dispatcher": "^3.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "extra": {
    "installer-paths": {
      "vendor/drupal/{$name}": ["drupal/core"],
      "vendor/drupal/hook_event_dispatcher": ["drupal/hook_event_dispatcher"]
    }
  }
}
